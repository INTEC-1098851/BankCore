<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2" />
    </startup>
    <system.serviceModel>
        <client>
            <endpoint address="https://bancore.azurewebsites.net/CoreWS.asmx"
                binding="basicHttpBinding" bindingConfiguration="CoreWSSoap"
                contract="WS.CoreWSSoap" name="CoreWSSoap" />
        </client>
        <bindings>
            <basicHttpBinding>
                <binding name="CoreWSSoap">
                    <security mode="Transport" />
                </binding>
                <binding name="CoreWSSoap1" />
            </basicHttpBinding>
        </bindings>
        
    </system.serviceModel>
  <appSettings>
    <add key="log4net.Internal.Debug" value="true"/>
  </appSettings>
  <system.diagnostics>
    <trace autoflush="true">
      <listeners>
        <add
        name="textWriterTraceListener"
        type="System.Diagnostics.TextWriterTraceListener"
        initializeData="C:\tmp\log4net.txt" />
      </listeners>
    </trace>
  </system.diagnostics>
</configuration>
